<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kt.mapdemo">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name=".MapDemoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".demo.plugin.cluster.MarkerClusterMapActivity"></activity>
        <activity android:name=".demo.start.MapCoordinationActivity" />
        <activity android:name=".demo.camera.MapGestureActivity" />
        <activity android:name=".demo.camera.MapCameraActivity" />
        <activity android:name=".demo.plugin.location.LocationMapActivity" />
        <activity android:name=".demo.plugin.symol.LabelTapActivity" />
        <activity android:name=".demo.plugin.style.DynamicStyleMapActivity" />
        <activity android:name=".demo.plugin.traffic.TrafficLayerActivity" />
        <activity android:name=".demo.overlay.PathOverlayActivity" />
        <activity android:name=".demo.overlay.InfowindowActivity" />
        <activity android:name=".demo.overlay.MarkerActivity" />
        <activity android:name=".demo.overlay.MapShapeOverlayActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".demo.style.MapLayerActivity" />
        <activity android:name=".demo.style.MapStyleActivity" />
        <activity android:name=".demo.start.MapFragmentActivity" />
        <activity android:name=".demo.start.MapViewActivity" />
        <activity android:name=".demo.start.MapFragmentCodeActivity" />
    </application>

</manifest>